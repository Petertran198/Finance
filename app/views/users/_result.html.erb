<% if @stock%>
 <div class="well results-block">
    <strong>Symbol: </strong> <%= @stock.ticker %>
    <strong>Name: </strong> <%= @stock.name %>
    <strong>Price: </strong> <%= @stock.last_price %>
    <%= link_to "Add to My Stocks", user_stocks_path(user: current_user, stock_ticker: @stock.ticker), class: "btn btn-md", method: :post %>
 </div>
<% end %>